{
  "name": "alcedo",
  "version": "3.0.0",
  "description": "A little Sass framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryangittings/alcedo"
  },
  "author": "Ryan Gittings",
  "license": "GPL-2.0 AND MIT",
  "homepage": "https://github.com/ryangittings/alcedo",
  "scripts": {
    "styles": "gulp --require @babel/register"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10"
  ],
  "vars": {
    "scssName": "test.scss",
    "cssName": "app.min.css"
  },
  "paths": {
    "src": {
      "scss": "./scss/"
    },
    "dist": {
      "css": "./css/"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-load-plugins": "^2.0.2",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5"
  }
}
